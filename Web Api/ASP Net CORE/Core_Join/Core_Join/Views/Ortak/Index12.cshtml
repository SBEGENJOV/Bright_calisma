@model IEnumerable<Core_Join.Models.ViewModels.elemanDepViewModel>

@{
    ViewData["Title"] = "Index";
}

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table table-dark">
    <thead>
        <tr>
            <th>
                Eleman No
            </th>
            <th>
                Eleman Adi
            </th>
            <th>
                Eleman Soyad
            </th>
            <th>
                Eleman Cinsiyet
            </th>
            <th>
                Eleman Adres
            </th>
            <th>
                Eleman Dep code
            </th>
            <th>
                Eleman Dep name
            </th>
            @*<th>
            Derartman Code
            </th>
            <th>
            Derartman Ad
            </th>*@
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.ElemanID
                </td>
                <td>
                    @item.Ad
                </td>
                <td>
                    @item.Soyad
                </td>
                <td>
                    @item.Cinsiyet
                </td>
                <td>
                    @item.Adres
                </td>
                <td>
                    @item.Code
                </td>
                <td>
                    @item.Name
                </td>
                <td>
                    <a asp-action="Edit" class="btn btn-warning" asp-route-id="@item.ElemanID">Edit</a> |
                    <a asp-action="Details" class="btn btn-success" asp-route-id="@item.ElemanID">Details</a> |
                    <a asp-action="Delete" class="btn btn-danger" asp-route-id="@item.ElemanID">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
<br />
