@model IEnumerable<mvcDapperProject.Models.FirmaModel>

@{
    ViewBag.Title = "Index";
}
<p>
    @Html.ActionLink("Yeni Ekle", "EY")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FirmaAd)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FirmaCiro)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Adres)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tel)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FirmaAd)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FirmaCiro)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Adres)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tel)
        </td>
        <td>
            @Html.ActionLink("Düzenle", "EY", new { id=item.FirmaNo }) |  
            @Html.ActionLink("Sil", "Delete", new { id = item.FirmaNo })
        </td>
    </tr>
}

</table>
